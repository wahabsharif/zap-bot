{
  "name": "zap-bot",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"wait-on http://localhost:3000 && electron ./src/main.js\"",
    "build": "next build",
    "web": "next dev",
    "start": "next start",
    "lint": "next lint",
    "electron": "concurrently \"next dev\" \"wait-on http://localhost:3000 && electron ./src/main.js\"",
    "build:electron": "next build && electron-builder"
  },
  "dependencies": {
    "2captcha": "^3.0.5-2",
