    "@types/mongoose": "^5.11.97",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.4",
    "bcrypt": "^5.1.1",
    "cheerio": "^1.0.0",
    "dotenv": "^16.4.5",
    "electron-reload": "^2.0.0-alpha.1",
    "express": "^4.19.2",
    "glob": "^9.3.5",
    "jimp": "^0.22.12",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^11.0.0",
    "mongoose": "^8.5.3",
    "next": "^14.2.5",
    "next-connect": "^1.0.0",
    "node-notifier": "^10.0.1",
    "puppeteer": "^23.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "rimraf": "^4.4.1",
    "selenium-webdriver": "^4.23.0",
    "sharp": "^0.33.4",
    "tesseract.js": "^5.1.0",
    "wait-on": "^7.2.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.3.0",
    "@types/puppeteer": "^7.0.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18",
    "@types/selenium-webdriver": "^4.1.25",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "concurrently": "^8.2.2",
    "electron": "^31.4.0",
    "electron-builder": "^24.13.3",
    "eslint": "^9.9.0",
    "eslint-config-next": "^14.2.5",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "build": {
    "appId": "com.yourcompany.zapbot",
    "productName": "Zap Bot",
    "files": [
      "out/",
      "src/main.js",
      "src/preload.js"
    ],
    "icon": "src/assets/logos/zap-bot-icon.ico",
    "mac": {
      "icon": "src/assets/logos/zap-bot-icon.ico"
    },
    "win": {
      "icon": "src/assets/logos/zap-bot-icon.ico"
    },
    "linux": {
      "icon": "src/assets/logos/zap-bot-icon.ico"
    }
